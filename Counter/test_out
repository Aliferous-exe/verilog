#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_011C7C80 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v011CE740_0 .var "clk", 0 0;
v011CE798_0 .var "reset", 0 0;
v011CE7F0_0 .net "value", 7 0, v011C2D08_0; 1 drivers
S_011C2C80 .scope module, "c1" "counter" 2 21, 3 1, S_011C7C80;
 .timescale 0 0;
P_011C917C .param/l "WIDTH" 3 2, +C4<01000>;
v011C7050_0 .net "clk", 0 0, v011CE740_0; 1 drivers
v011C2D08_0 .var "out", 7 0;
v011C2D60_0 .net "reset", 0 0, v011CE798_0; 1 drivers
E_011C9078 .event posedge, v011C7050_0;
    .scope S_011C2C80;
T_0 ;
    %wait E_011C9078;
    %load/v 8, v011C2D60_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v011C2D08_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v011C2D08_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %ix/load 0, 8, 0;
    %assign/v0 v011C2D08_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_011C7C80;
T_1 ;
    %set/v v011CE798_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_011C7C80;
T_2 ;
    %vpi_call 2 6 "$dumpfile", "test.vcd";
    %vpi_call 2 7 "$dumpvars", 1'sb0, S_011C7C80;
    %delay 17, 0;
    %set/v v011CE798_0, 1, 1;
    %delay 11, 0;
    %set/v v011CE798_0, 0, 1;
    %delay 29, 0;
    %set/v v011CE798_0, 1, 1;
    %delay 5, 0;
    %set/v v011CE798_0, 0, 1;
    %delay 513, 0;
    %vpi_call 2 13 "$finish";
    %end;
    .thread T_2;
    .scope S_011C7C80;
T_3 ;
    %set/v v011CE740_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_011C7C80;
T_4 ;
    %delay 1, 0;
    %load/v 8, v011CE740_0, 1;
    %inv 8, 1;
    %set/v v011CE740_0, 8, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_011C7C80;
T_5 ;
    %vpi_call 2 24 "$monitor", "At time %t, value = %h (%0d)", $time, v011CE7F0_0, v011CE7F0_0;
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "countertb.v";
    "counter.v";
