:vpi_module "va_math";
S_011580A8 .scope module, "muxtest" "muxtest" 2 1;
 .timescale 0 0;
v01152CD8_0 .var "A", 15 0;
v01152D30_0 .net "F", 0 0, L_011582F8; 1 drivers
v01152D88_0 .var "S", 3 0;
S_011574F8 .scope module, "M" "mux16to1" 2 6, 3 1, S_011580A8;
 .timescale 0 0;
v01157580_0 .net "in", 15 0, v01152CD8_0; 1 drivers
v011575D8_0 .alias "out", 0 0, v01152D30_0;
v01152C80_0 .net "sel", 3 0, v01152D88_0; 1 drivers
L_011582F8 .part/v v01152CD8_0, v01152D88_0, 1;
    .scope S_011580A8;
T_0 ;
    %vpi_call 2 9 "$dumpfile", "mux16to1.vcd";
    %vpi_call 2 10 "$dumpvars", 1'sb0, S_011580A8;
    %vpi_call 2 11 "$monitor", $time, " A=%h, S=%h, F=%b", v01152CD8_0, v01152D88_0, v01152D30_0;
    %delay 5, 0;
    %movi 8, 16138, 16;
    %set/v v01152CD8_0, 8, 16;
    %set/v v01152D88_0, 0, 4;
    %delay 5, 0;
    %movi 8, 1, 4;
    %set/v v01152D88_0, 8, 4;
    %delay 5, 0;
    %movi 8, 6, 4;
    %set/v v01152D88_0, 8, 4;
    %delay 5, 0;
    %movi 8, 12, 4;
    %set/v v01152D88_0, 8, 4;
    %delay 5, 0;
    %vpi_call 2 17 "$finish";
    %end;
    .thread T_0;
