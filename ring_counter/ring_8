#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01123800 .scope module, "tb_ring_counter_8bit" "tb_ring_counter_8bit" 2 2;
 .timescale -9 -12;
v01124760_0 .var "clk", 0 0;
v0112EEC8_0 .net "out", 7 0, v011246B0_0; 1 drivers
v0112EF20_0 .var "rst", 0 0;
S_01123888 .scope module, "dut" "ring_counter_8bit" 2 8, 3 1, S_01123800;
 .timescale 0 0;
v01124658_0 .net "clk", 0 0, v01124760_0; 1 drivers
v011246B0_0 .var "out", 7 0;
v01124708_0 .net "rst", 0 0, v0112EF20_0; 1 drivers
E_01161E90 .event posedge, v01124708_0, v01124658_0;
    .scope S_01123888;
T_0 ;
    %wait E_01161E90;
    %load/v 8, v01124708_0, 1;
    %jmp/0xz  T_0.0, 8;
    %movi 8, 1, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v011246B0_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %ix/load 1, 7, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.2, 4;
    %load/x1p 16, v011246B0_0, 1;
    %jmp T_0.3;
T_0.2 ;
    %mov 16, 2, 1;
T_0.3 ;
    %mov 8, 16, 1; Move signal select into place
    %load/v 9, v011246B0_0, 7; Select 7 out of 8 bits
    %ix/load 0, 8, 0;
    %assign/v0 v011246B0_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_01123800;
T_1 ;
    %set/v v01124760_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_01123800;
T_2 ;
    %delay 5000, 0;
    %load/v 8, v01124760_0, 1;
    %inv 8, 1;
    %set/v v01124760_0, 8, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_01123800;
T_3 ;
    %vpi_call 2 19 "$dumpfile", "ringcounter8bit.vcd";
    %vpi_call 2 20 "$dumpvars", 1'sb0, S_01123800;
    %set/v v0112EF20_0, 1, 1;
    %delay 12000, 0;
    %set/v v0112EF20_0, 0, 1;
    %delay 200000, 0;
    %vpi_call 2 29 "$finish";
    %end;
    .thread T_3;
    .scope S_01123800;
T_4 ;
    %vpi_call 2 33 "$monitor", "%0t  clk=%b  rst=%b  out=%b", $time, v01124760_0, v0112EF20_0, v0112EEC8_0;
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_ring_counter_8bit.v";
    "ring_counter_8bit.v";
