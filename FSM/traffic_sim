#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01257530 .scope module, "tb_traffic_lights" "tb_traffic_lights" 2 1;
 .timescale 0 0;
v011337A0_0 .var "clk", 0 0;
v011337F8_0 .net "lights", 2 0, v012543A0_0; 1 drivers
v01133850_0 .var "reset", 0 0;
S_01256F30 .scope module, "uut" "traffic_lights" 2 5, 3 1, S_01257530;
 .timescale 0 0;
P_01256FBC .param/l "GREEN" 3 9, C4<101>;
P_01256FD0 .param/l "RED" 3 9, C4<100>;
P_01256FE4 .param/l "S0" 3 6, C4<00>;
P_01256FF8 .param/l "S1" 3 6, C4<01>;
P_0125700C .param/l "S2" 3 6, C4<10>;
P_01257020 .param/l "YELLOW" 3 9, C4<001>;
v01254348_0 .net "clk", 0 0, v011337A0_0; 1 drivers
v012543A0_0 .var "lights", 2 0;
v012543F8_0 .var "next_state", 1 0;
v011336F0_0 .net "reset", 0 0, v01133850_0; 1 drivers
v01133748_0 .var "state", 1 0;
E_01258C80 .event edge, v01133748_0;
E_01258DC0 .event posedge, v011336F0_0, v01254348_0;
    .scope S_01256F30;
T_0 ;
    %wait E_01258DC0;
    %load/v 8, v011336F0_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 2, 0;
    %assign/v0 v01133748_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v012543F8_0, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v01133748_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_01256F30;
T_1 ;
    %wait E_01258C80;
    %load/v 8, v01133748_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_1.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_1.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_1.2, 6;
    %movi 8, 4, 3;
    %set/v v012543A0_0, 8, 3;
    %set/v v012543F8_0, 0, 2;
    %jmp T_1.4;
T_1.0 ;
    %movi 8, 4, 3;
    %set/v v012543A0_0, 8, 3;
    %movi 8, 1, 2;
    %set/v v012543F8_0, 8, 2;
    %jmp T_1.4;
T_1.1 ;
    %movi 8, 5, 3;
    %set/v v012543A0_0, 8, 3;
    %movi 8, 2, 2;
    %set/v v012543F8_0, 8, 2;
    %jmp T_1.4;
T_1.2 ;
    %movi 8, 1, 3;
    %set/v v012543A0_0, 8, 3;
    %set/v v012543F8_0, 0, 2;
    %jmp T_1.4;
T_1.4 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_01257530;
T_2 ;
    %vpi_call 2 8 "$dumpfile", "trafficlights.vcd";
    %vpi_call 2 9 "$dumpvars", 1'sb0, S_01257530;
    %set/v v011337A0_0, 0, 1;
    %set/v v01133850_0, 1, 1;
    %delay 10, 0;
    %set/v v01133850_0, 0, 1;
    %delay 200, 0;
    %vpi_call 2 15 "$finish";
    %end;
    .thread T_2;
    .scope S_01257530;
T_3 ;
    %delay 5, 0;
    %load/v 8, v011337A0_0, 1;
    %inv 8, 1;
    %set/v v011337A0_0, 8, 1;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
